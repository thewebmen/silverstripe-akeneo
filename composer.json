{
  "name": "wedevelopnl/silverstripe-akeneo",
  "description": "akeneo module for silverstripe",
  "keywords": [
    "akeneo",
    "pim"
  ],
  "type": "silverstripe-vendormodule",
  "require": {
    "php": "^8.0",
    "silverstripe/framework": "^4.11",
    "silverstripe/cms": "^4.11",
    "symbiote/silverstripe-gridfieldextensions": "^3.4",
    "undefinedoffset/sortablegridfield": "^2.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4"
  },
  "extra": {
    "installer-name": "silverstripe-akeneo",
    "expose": [
      "images"
    ]
  },
  "autoload": {
    "psr-4": {
      "WeDevelop\\Akeneo\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": false,
      "silverstripe/vendor-plugin": false
    }
  }
}
